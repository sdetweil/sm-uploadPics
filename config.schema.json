{
	"schema": {
		"uploadPics": {
			"type": "object",
			"title": "{{title}}",
			"properties": {
				"dest": {
					"type": "string",
					"title": "{{dest-title}}",
					"default": "backgroundImage/images"
				},
				"debug": {
					"type": "boolean",
					"title": "{{debug-title}}",
